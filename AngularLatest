=========================apply-debit-card.component.ts
import { Component } from '@angular/core';
import { DebitCardService } from '../debit-card.service';

@Component({
  selector: 'app-apply-debit-card',
  templateUrl: './apply-debit-card.component.html'
})
export class ApplyDebitCardComponent {
  debitCardApplication = {
    accountNo: null,
    customerId: null,
    status: 'Applied'
  };

  constructor(private debitCardService: DebitCardService) { }

  applyDebitCard() {
    this.debitCardService.applyDebitCard(this.debitCardApplication).subscribe(
      response => {
        console.log('Debit card applied successfully', response);
      },
      error => {
        console.error('Error applying debit card', error);
      }
    );
  }
}
=============HTML===========
<form (submit)="applyDebitCard()">
  <label>Account Number:</label>
  <input type="number" [(ngModel)]="debitCardApplication.accountNo" name="accountNo" required>
  <label>Customer ID:</label>
  <input type="number" [(ngModel)]="debitCardApplication.customerId" name="customerId" required>
  <button type="submit">Apply for Debit Card</button>
</form>
==================================Allot DEBIT CRAD COMPONENT===================
import { Component } from '@angular/core';
import { DebitCardService } from '../debit-card.service';

@Component({
  selector: 'app-allot-debit-card',
  templateUrl: './allot-debit-card.component.html'
})
export class AllotDebitCardComponent {
  dcApplicationId: number;
  dcDetail = {
    debitCardNo: null,
    accountNo: null,
    cvv: null,
    expiryDate: null
  };

  constructor(private debitCardService: DebitCardService) { }

  allotDebitCard() {
    this.debitCardService.allotDebitCard(this.dcApplicationId, this.dcDetail).subscribe(
      response => {
        console.log('Debit card alloted successfully', response);
      },
      error => {
        console.error('Error allotting debit card', error);
      }
    );
  }
}
=================================Allot DEBIT CARD HTML======================
<form (submit)="allotDebitCard()">
  <label>Application ID:</label>
  <input type="number" [(ngModel)]="dcApplicationId" name="dcApplicationId" required>
  <label>Account Number:</label>
  <input type="number" [(ngModel)]="dcDetail.accountNo" name="accountNo" required>
  <button type="submit">Allot Debit Card</button>
</form>
==================================debitcard details==============
import { Component } from '@angular/core';
import { DebitCardService } from '../debit-card.service';

@Component({
  selector: 'app-view-debit-card-details',
  templateUrl: './view-debit-card-details.component.html'
})
export class ViewDebitCardDetailsComponent {
  accountNumber: number;
  debitCardDetails: any;

  constructor(private debitCardService: DebitCardService) { }

  getDebitCardDetails() {
    this.debitCardService.getDebitCardDetails(this.accountNumber).subscribe(
      response => {
        this.debitCardDetails = response;
        console.log('Debit card details retrieved successfully', response);
      },
      error => {
        console.error('Error retrieving debit card details', error);
      }
    );
  }
}
===================HTML=======================
<form (submit)="getDebitCardDetails()">
  <label>Account Number:</label>
  <input type="number" [(ngModel)]="accountNumber" name="accountNumber" required>
  <button type="submit">Get Debit Card Details</button>
</form>
<div *ngIf="debitCardDetails">
  <div *ngFor="let card of debitCardDetails">
    <p>Debit Card Number: {{ card.debitCardNo }}</p>
    <p>CVV: {{ card.cvv }}</p>
    <p>Expiry Date: {{ card.expiryDate }}</p>
  </div>
</div>
=====================LISt
import { Component } from '@angular/core';
import { DebitCardService } from '../debit-card.service';

@Component({
  selector: 'app-list-debit-cards',
  templateUrl: './list-debit-cards.component.html'
})
export class ListDebitCardsComponent {
  customerId: number;
  debitCardApplications: any;

  constructor(private debitCardService: DebitCardService) { }

  listDebitCards() {
    this.debitCardService.listDebitCards(this.customerId).subscribe(
      response => {
        this.debitCardApplications = response;
        console.log('Debit card applications retrieved successfully', response);
      },
      error => {
        console.error('Error retrieving debit card applications', error);
      }
    );
  }
}
============List HTML==================
<form (submit)="listDebitCards()">
  <label>Customer ID:</label>
  <input type="number" [(ngModel)]="customerId" name="customerId" required>
  <button type="submit">List Debit Cards</button>
</form>
<div *ngIf="debitCardApplications">
  <div *ngFor="let application of debitCardApplications">
    <p>Application ID: {{ application.dcapplicationId }}</p>
    <p>Status: {{ application.status }}</p>
    <div *ngFor="let card of application.debitCardDetails">
      <p>Debit Card Number: {{ card.debitCardNo }}</p>
      <p>CVV: {{ card.cvv }}</p>
      <p>Expiry Date: {{ card.expiryDate }}</p>
    </div>
  </div>
</div>
========================app.routing
import { NgModule } from '@angular/core';
import { RouterModule, Routes } from '@angular/router';
import { ApplyDebitCardComponent } from './apply-debit-card/apply-debit-card.component';
import { AllotDebitCardComponent } from './allot-debit-card/allot-debit-card.component';
import { ViewDebitCardDetailsComponent } from './view-debit-card-details/view-debit-card-details.component';
import { ListDebitCardsComponent } from './list-debit-cards/list-debit-cards.component';

const routes: Routes = [
  { path: 'apply-debit-card', component: ApplyDebitCardComponent },
  { path: 'allot-debit-card', component: AllotDebitCardComponent },
  { path: 'view-debit-card-details', component: ViewDebitCardDetailsComponent },
  { path: 'list-debit-cards', component: ListDebitCardsComponent },
  { path: '', redirectTo: '/apply-debit-card', pathMatch: 'full' }
];

@NgModule({
  imports: [RouterModule.forRoot(routes)],
  exports: [RouterModule]
})
export class AppRoutingModule { }
